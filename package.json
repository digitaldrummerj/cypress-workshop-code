{
  "name": "cypress-workshop-code",
  "version": "1.0.0",
  "description": "Testing Vue + Vuex + REST TodoMVC using Cypress",
  "scripts": {
    "start": "npx json-server --static . data.json --middlewares ./node_modules/json-server-reset",
    "reset": "node reset-db.js",
    "reset:db": "npm run reset",
    "reset:database": "npm run reset",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "prettier-standard 'cypress/**/*.js'",
    "lint": "standard 'cypress/**/*.js'"
  },
  "dependencies": {
    "cypress": "3.4.1",
    "json-server": "0.15.1",
    "json-server-reset": "1.2.0",
    "@cypress/code-coverage": "1.10.1",
    "istanbul-lib-coverage": "2.0.5",
    "cypress-dark": "1.7.14",
    "cypress-failed-log": "2.5.1",
    "cypress-pipe": "1.5.0",
    "mocha": "6.2.0",
    "mocha-junit-reporter": "1.23.1",
    "mocha-multi-reporters": "1.1.7",
    "mochawesome": "3.1.1",
    "mochawesome-merge": "1.0.4",
    "mochawesome-report-generator": "4.0.1",
    "nyc": "14.1.1",
    "start-server-and-test": "1.10.5",
    "prettier": "1.18.2",
    "prettier-standard": "15.0.1"
  },
  "standard": {
    "ignore": [
      "cypress/integration/13-app-actions/todo-page-object.js",
      "cypress/integration/16-preprocessors/spec.js"
    ],
    "globals": [
      "Cypress",
      "cy",
      "it",
      "describe",
      "context",
      "beforeEach",
      "assert",
      "expect"
    ]
  }
}
